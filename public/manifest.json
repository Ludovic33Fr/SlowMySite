{
  "manifest_version": 2,
  "name": "Slow-My-site",
  "version": "0.1.0",
  "description": "Enable to test the integration of bad javascript into your website",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Slow-My-site",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://localhost/",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}